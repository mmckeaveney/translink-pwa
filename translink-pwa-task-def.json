{
  "family": "translink-pwa-task",
  "executionRoleArn": "arn:aws:iam::850494299305:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256", 
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "translink-pwa-task",
      "image": "frankblizzard/translink-pwa",
      "cpu": 20,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
